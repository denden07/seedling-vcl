---
metadata:
  partial_name: Lib | Validations | Truthy
---
{% comment %}
  params: @object
          @field_name
          @c
          @key[optional]
{% endcomment %}

{% liquid
  unless object[field_name]
    assign message = message | default: "not true"
    function c = 'modules/dashboard/lib/register_error', contract: c, field_name: field_name, message: message
  endunless
  return c
%}
